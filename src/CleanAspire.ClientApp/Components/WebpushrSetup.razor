@code {
    protected override async Task OnAfterRenderAsync(bool firstRender)
    {
        if (firstRender)
        {
            var publicKey = await WebpushrService.GetPublicKeyAsync();
            var webpushr = new Webpushr(JS);
            await webpushr.SetupWebpushrAsync(publicKey!);
        }
    }
}
